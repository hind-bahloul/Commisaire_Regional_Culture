<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Associations">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <HorizontalScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="230dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img1"
                                        android:layout_width="150dp"
                                        android:layout_height="200dp"
                                        app:srcCompat="@mipmap/ass3" />

                                    <TextView
                                        android:id="@+id/textView33"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="جمعية العمل التطوعي"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="230dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img2"
                                        android:layout_width="150dp"
                                        android:layout_height="200dp"
                                        app:srcCompat="@mipmap/as2" />

                                    <TextView
                                        android:id="@+id/textView3"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="جمعية التواصل البيئة و التنمية"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="230dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img3"
                                        android:layout_width="150dp"
                                        android:layout_height="200dp"
                                        app:srcCompat="@mipmap/as" />

                                    <TextView
                                        android:id="@+id/textView32"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="شبكة غصن الزيتون للجمعيات التنموية بصفاقس"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="230dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img4"
                                        android:layout_width="150dp"
                                        android:layout_height="180dp"
                                        app:srcCompat="@mipmap/ass6" />

                                    <TextView
                                        android:id="@+id/textView03"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="جمعية بيت الخبرة"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="220dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img5"
                                        android:layout_width="150dp"
                                        android:layout_height="180dp"
                                        app:srcCompat="@mipmap/ass5" />

                                    <TextView
                                        android:id="@+id/textView34"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="جمعية نقابة التوجيه السياحي بصفاقس"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="220dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img6"
                                        android:layout_width="150dp"
                                        android:layout_height="180dp"
                                        app:srcCompat="@mipmap/as4" />

                                    <TextView
                                        android:id="@+id/textView320"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="جمعية حماية البيئة والطبيعة"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="220dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img7"
                                        android:layout_width="150dp"
                                        android:layout_height="180dp"
                                        app:srcCompat="@mipmap/ass9" />

                                    <TextView
                                        android:id="@+id/textView02"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="جمعية تنمية التعاون بين مدينتي صفاقس وماربورغ"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="220dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img8"
                                        android:layout_width="150dp"
                                        android:layout_height="180dp"
                                        app:srcCompat="@mipmap/ass8" />

                                    <TextView
                                        android:id="@+id/textView04"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="جمعية العمل الايجابي"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="150dp"
                                    android:layout_height="220dp"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/img9"
                                        android:layout_width="150dp"
                                        android:layout_height="180dp"
                                        app:srcCompat="@mipmap/ass7" />

                                    <TextView
                                        android:id="@+id/textView06"
                                        android:layout_width="150dp"
                                        android:layout_height="30dp"
                                        android:gravity="center"
                                        android:text="جمعية التنمية المستديمة بولاية صفاقس"
                                        android:textColor="@color/common_google_signin_btn_text_dark_focused"
                                        android:textSize="12sp" />

                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </LinearLayout>
                </HorizontalScrollView>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>